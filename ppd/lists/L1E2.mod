int P = 20; //
int k_cnt = 5; //
int obj = 7;
int l[1..20][1..2] = [[24, 46], [15, 40], [2, 29], [47, 17], [26, 29], [2, 30], [43, 25], [25, 36], [47, 9], [0, 43], [4, 18], [6, 3], [21, 10], [47, 25], [50, 33], [2, 29], [16, 44], [37, 15], [42, 34], [35, 40]];
int k[1..5][1..2] = [[17, 6], [34, 50], [33, 38], [46, 4], [31, 31]];
 
dvar boolean points[1..P];

minimize 
	sum(i in 1..P) 
		points[i]*
			(sum(_k in 1..k_cnt)
				sqrt(
					(l[i][1] - k[_k][1])^2+
					(l[i][2] - k[_k][2])^2
					)
			);


subject to {
  sum(i in 1..P) points[i] == obj;
}